def comparison_test(x: int, y: int): int => { 
     printi( x == y) // prints 1
     printi( x != y) // prints 0
     printi( x >= y) // prints 1
     printi( x <= y) // prints 1
     printi( x > y)  // prints 0
     printi( x < y)  // prints 0
     return x < y    // return 0
}

echo(comparison_test(10,10)) // prints 0